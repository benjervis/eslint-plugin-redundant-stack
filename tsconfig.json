{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "strict": true,
    "lib": ["es2019"],
    "target": "es5"
  },
  "include": ["./src"]
}
